<c74object name='spt.attractor.clorenz' category='chaos,  simulation'>

	<digest>Coupled Lorenz Attractor - 6-scroll multiscroll chaos </digest>
	<description>Coupled Lorenz Attractor - 6-scroll multiscroll chaos </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Max Worgan </metadata>
		<metadata name='tag'>chaos</metadata>
		<metadata name='tag'>simulation</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='bang'>
			<digest>Reset to initial state </digest>
			<description>Reset to initial state </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='beta' get='1' set='1' type='float64' size='1' >
			<digest>Geometric factor (typical: 8/3 = 2</digest>
			<description>Geometric factor (typical: 8/3 = 2.667) </description>
		</attribute>

		<attribute name='dt' get='1' set='1' type='float64' size='1' >
			<digest>Base integrator time step </digest>
			<description>Base integrator time step </description>
		</attribute>

		<attribute name='epsilon' get='1' set='1' type='float64' size='1' >
			<digest>Coupling strength (controls multi-scroll behavior) </digest>
			<description>Coupling strength (controls multi-scroll behavior) </description>
		</attribute>

		<attribute name='position' get='1' set='1' type='float64' size='1' >
			<digest>Reset position on bad state </digest>
			<description>Reset position on bad state </description>
		</attribute>

		<attribute name='r1' get='1' set='1' type='float64' size='1' >
			<digest>Rayleigh number for driving oscillator (typical: 28) </digest>
			<description>Rayleigh number for driving oscillator (typical: 28) </description>
		</attribute>

		<attribute name='r2' get='1' set='1' type='float64' size='1' >
			<digest>Rayleigh number for coupled oscillator (typical: 28) </digest>
			<description>Rayleigh number for coupled oscillator (typical: 28) </description>
		</attribute>

		<attribute name='scale_outputs' get='1' set='1' type='long' size='1' >
			<digest>Scale outputs to +/-1 </digest>
			<description>Scale outputs to +/-1 </description>
		</attribute>

		<attribute name='sigma' get='1' set='1' type='float64' size='1' >
			<digest>Prandtl number (typical: 10) </digest>
			<description>Prandtl number (typical: 10) </description>
		</attribute>

		<attribute name='smooth' get='1' set='1' type='float64' size='1' >
			<digest>Output smoothing (0=off, 1=strong) </digest>
			<description>Output smoothing (0=off, 1=strong) </description>
		</attribute>

		<attribute name='speed_primary' get='1' set='1' type='float64' size='1' >
			<digest>Primary substep multiplier </digest>
			<description>Primary substep multiplier </description>
		</attribute>

		<attribute name='speed_secondary' get='1' set='1' type='float64' size='1' >
			<digest>Secondary substep multiplier </digest>
			<description>Secondary substep multiplier </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
		<seealso name='spt.attractor.lorenz' />
		<seealso name='spt.attractor.dadras' />
		<seealso name='spt.attractor.thomas' />
	</seealsolist>


</c74object>
