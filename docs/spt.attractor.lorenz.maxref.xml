<c74object name='spt.attractor.lorenz' category='chaos,  simulation'>

	<digest>Lorenz Attractor simulation </digest>
	<description>Lorenz Attractor simulation </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Max Worgan </metadata>
		<metadata name='tag'>chaos</metadata>
		<metadata name='tag'>simulation</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='bang'>
			<digest>Reset attractor to initial state </digest>
			<description>Reset attractor to initial state </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='beta' get='1' set='1' type='float64' size='1' >
			<digest>Parameter beta </digest>
			<description>Parameter beta </description>
		</attribute>

		<attribute name='dt' get='1' set='1' type='float64' size='1' >
			<digest>Base integrator time step </digest>
			<description>Base integrator time step </description>
		</attribute>

		<attribute name='position' get='1' set='1' type='float64' size='1' >
			<digest>Reset position (x, y, z) </digest>
			<description>Reset position (x, y, z) </description>
		</attribute>

		<attribute name='rho' get='1' set='1' type='float64' size='1' >
			<digest>Parameter rho </digest>
			<description>Parameter rho </description>
		</attribute>

		<attribute name='scale_outputs' get='1' set='1' type='long' size='1' >
			<digest>Scale outputs to +/-1 </digest>
			<description>Scale outputs to +/-1 </description>
		</attribute>

		<attribute name='sigma' get='1' set='1' type='float64' size='1' >
			<digest>Parameter sigma </digest>
			<description>Parameter sigma </description>
		</attribute>

		<attribute name='smooth' get='1' set='1' type='float64' size='1' >
			<digest>Output smoothing (0=off, 1=max) </digest>
			<description>Output smoothing (0=off, 1=max) </description>
		</attribute>

		<attribute name='speed_primary' get='1' set='1' type='float64' size='1' >
			<digest>Primary substep multiplier </digest>
			<description>Primary substep multiplier </description>
		</attribute>

		<attribute name='speed_secondary' get='1' set='1' type='float64' size='1' >
			<digest>Secondary substep multiplier </digest>
			<description>Secondary substep multiplier </description>
		</attribute>

		<attribute name='use_rk4' get='1' set='1' type='long' size='1' >
			<digest>Use RK4 integrator (more accurate, slower) </digest>
			<description>Use RK4 integrator (more accurate, slower) </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
