<c74object name='spt.attractor.tsucs1' category='chaos,  simulation'>

	<digest>Three-Scroll Unified Chaotic System (TSUCS1) </digest>
	<description>Three-Scroll Unified Chaotic System (TSUCS1) </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Max Worgan </metadata>
		<metadata name='tag'>chaos</metadata>
		<metadata name='tag'>simulation</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='bang'>
			<digest>Reset attractor to initial state </digest>
			<description>Reset attractor to initial state </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='alpha' get='1' set='1' type='float64' size='1' >
			<digest>Parameter alpha (0-1) - interpolates between Lorenz and Lu Chen attractors </digest>
			<description>Parameter alpha (0-1) - interpolates between Lorenz and Lu Chen attractors </description>
		</attribute>

		<attribute name='dt' get='1' set='1' type='float64' size='1' >
			<digest>Base integrator time step </digest>
			<description>Base integrator time step </description>
		</attribute>

		<attribute name='log_resets' get='1' set='1' type='long' size='1' >
			<digest>Log resets to the console </digest>
			<description>Log resets to the console </description>
		</attribute>

		<attribute name='m' get='1' set='1' type='float64' size='1' >
			<digest>Parameter m (-250-250) - controls morphology </digest>
			<description>Parameter m (-250-250) - controls morphology </description>
		</attribute>

		<attribute name='position' get='1' set='1' type='float64' size='1' >
			<digest>Reset position (x, y, z) </digest>
			<description>Reset position (x, y, z) </description>
		</attribute>

		<attribute name='scale_outputs' get='1' set='1' type='long' size='1' >
			<digest>Scale outputs to +/-1 </digest>
			<description>Scale outputs to +/-1 </description>
		</attribute>

		<attribute name='smooth' get='1' set='1' type='float64' size='1' >
			<digest>Output smoothing (0=off, 1=max) </digest>
